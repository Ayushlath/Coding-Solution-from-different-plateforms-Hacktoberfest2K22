
#include<bits/stdc++.h>
using namespace std;

  


class Solution{
  public:
    long long int floorSqrt(long long int x) 
    {
        int s=0;
        long long int e=x,ans=1;
        long long int mid=s+(e-s)/2;
        if(x==1)
        {
            return 1;
        }
        while(s<=e)
        {
            long long int square=mid*mid;
            if(square==x)
            {
                return mid;
            }
            else if(square<=x)
            {
                ans=mid;
                s=mid+1;
                
            }
            else{
                e=mid-1;
            }
        
            mid=s+(e-s)/2;
        }
    
        return ans;
    }
};



int main()
{
	int t;
	cin>>t;
	while(t--)
	{
		long long n;
		cin>>n;
		Solution obj;
		cout << obj.floorSqrt(n) << endl;
	}
    return 0;   
}

